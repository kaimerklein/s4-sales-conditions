{
  "_version": "1.49.0",
  "sap.app": {
    "id": "salesConditions",
    "type": "application",
    "title": "Sales Price Conditions",
    "description": "List Report for Sales Price Conditions",
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/sales-condition/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui5": {
    "dependencies": {
      "minUI5Version": "1.120.0",
      "libs": {
        "sap.fe.templates": {}
      }
    },
    "models": {
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "routing": {
      "routes": [
        {
          "pattern": ":?query:",
          "name": "ConditionRecordsList",
          "target": "ConditionRecordsList"
        }
      ],
      "targets": {
        "ConditionRecordsList": {
          "type": "Component",
          "id": "ConditionRecordsList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "entitySet": "ConditionRecords",
              "initialLoad": "Enabled",
              "variantManagement": "None"
            }
          }
        }
      }
    }
  }
}
